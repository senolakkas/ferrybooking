<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ByteCore.FerryBooking.Core" namespace="ByteCore.FerryBooking.Core">
  <class lazy="false" name="RouteOrderDetail" table="RouteOrderDetail" >
    <id name="ID" type="System.Int32" column="RouteOrderDetailId">
        <generator class="identity"/>
    </id>
    <property name="FareTypeId" column="FareTypeId" type="System.Int32" not-null="false" />
    <property name="RouteOrderID" column="RouteOrderID" type="System.Int32" not-null="false" />
    <property name="Quantity" column="Quantity" type="System.Int32" not-null="false" />
    <property name="Amount" column="Amount" type="System.Decimal" not-null="false" />
    <many-to-one name="FareType" column="FareTypeId" class="FareType"  update="false"  insert="false" />
    <many-to-one name="RouteOrder" column="RouteOrderID" class="RouteOrder"  update="false"  insert="false" />
    <bag name="RouteOrderVehicleDetails" table="RouteOrderVehicleDetail" inverse="true" lazy="true" cascade="none">
      <key column="RouteOrderDetailId" />
      <one-to-many class="RouteOrderVehicleDetail"/>
    </bag>
 </class>
</hibernate-mapping>
